    "lint": "pnpm lint:code && pnpm lint:styles && pnpm lint:types",
    "story:dev": "histoire dev --port 3001",
    "story:build": "histoire build",
    "story:preview": "histoire preview",
    "openapi-generate": "openapi-typescript http://80.78.240.164:8890/openapi.json -o ./src/shared/api/schema/index.d.ts && prettier --write src/shared/api/schema/",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky install"
  },
  "dependencies": {
    "@formkit/auto-animate": "^0.8.1",
    "@sentry/vue": "^7.90.0",
